<div class="col-md-10 col-md-offset-1" id="element1"></div>
  <div class="col-md-6 col-md-offset-3"id="content1"></div>
  <div class="col-md-4 col-md-offset-4" id="signup">
    <img src="clothing1.svg" id="hanger">
    <h1>ENSEMBLE.</h1>
    <h2>Sign up now and start rating<h2>
    <form action="/users" method="post"  id="registernew">
      <input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden"/>
      Username:<br>
      <input type="text" name="user[username]" placeholder="username">
      <br>
      Email:<br>
      <input type="text" name="user[email]" placeholder="email">
      <br>
      Password:<br>
      <input type="password" name="user[password]" placeholder="password">
      <br>
      Confirm Password:<br>
      <input type="password" name="user[confirm_password]" placeholder="confirm password">
      <br><br>
      <input type="submit" value="Submit">
    </form>
    <div class="instagram">Or <a href="/instagram/oauth/connect" id="withinstagram">Login With Instagram <i class="fa fa-instagram"></i></a>
    </div>
