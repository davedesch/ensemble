<button><a href="/instagram/recent">Get my pix from instagram</a></button>
<script id="all-outfits-template" type="text/x-handlebars-template">
  {{#each allOutfits}}
    <img src="{{image}}" class="img-responsive col-12" id="outfitpicture" alt="Your Ensemble!">
    <div class="col-12">{{title}}</div>
    <div class="col-12">{{types}}</div>
    <div class="col-12 rating">
      <div class="rating-star"></div>
      <div class="rating-star"></div>
      <div class="rating-star"></div>
      <div class="rating-star"></div>
      <div class="rating-star"></div>
      <button type="button" id='rate-this'>Rate This</button>
    </div>
    <div id='rating-form'>
      <form action="/users/+"session[:id]"+/ensembles/:outfit_id/reviews">
        <textarea rows="4" cols="50">Leave a fuckin nice Review..</textarea><br>
        <input type="submit" value='Hang It'>
      </form>
    </div>
     <div class="col-12">{{caption}}</div>
  {{/each}}
</script>

<script id="recent-outfits-template" type="text/x-handlebars-template">
  {{#each recentOutfits}}
    <div class="container">
      <img src={{image}} class="img-thumbnail col-md-3" alt="Your Ensemble!">
      <div class="col-md-9">{{title}}, Rating: {{avg_rating}}</div>
    </div>
  {{/each}}
</script>

<script id="trending-hashtags-template" type="text/x-handlebars-template">
  {{#each trendingHashtags}}
    <div class="container">
      <div class="col-md-10">#{{data.author}}</div>
      <div class="col-md-2">{{data.num_comments}}</div>
    </div>
  {{/each}}
</script>


