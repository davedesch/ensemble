<button><a href="/instagram/recent">Get my pix from instagram</a></button>
<script id="all-outfits-template" type="text/x-handlebars-template">
  {{#each allOutfits}}
    <img src={{data.thumbnail}} class="img-responsive col-12" id="outfitpicture" alt="Your Ensemble!">
    <div class="col-12">{{data.url}}</div>
    <div class="col-12 rating">
      <div class="rating-star"></div>
      <div class="rating-star"></div>
      <div class="rating-star"></div>
      <div class="rating-star"></div>
      <div class="rating-star"></div>
      <button type="button" id='rate-this'>Rate This</button>
    </div>
    <div id='rating-form'>
      <form action="/users/+"session[:id]"+/ensembles/:outfit_id/reviews">
        <textarea rows="4" cols="50">Leave a fuckin nice Review..</textarea><br>
        <input type="submit" value='Hang It'>
      </form>
    </div>
    <div class="col-12">{{data.num_comments}}</div>
     <div class="col-12">{{data.name}}</div>
  {{/each}}
</script>

<script id="recent-outfits-template" type="text/x-handlebars-template">
  {{#each recentOutfits}}
    <div class="container">
      <img src={{data.thumbnail}} class="img-thumbnail col-md-3" alt="Your Ensemble!">
      <div class="col-md-9">{{data.title}}, Score: {{data.score}}</div>
    </div>
  {{/each}}
</script>

<script id="trending-hashtags-template" type="text/x-handlebars-template">
  {{#each trendingHashtags}}
    <div class="container">
      <div class="col-md-10">#{{data.author}}</div>
      <div class="col-md-2">{{data.num_comments}}</div>
    </div>
  {{/each}}
</script>

<html>
<head>
  <title>ensemble</title>
  <script src="https://code.jquery.com/jquery-2.1.3.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/3.0.0/handlebars.min.js"></script>

  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap-theme.min.css">
  <link type="text/css" rel="stylesheet" href="dashboard.css"/>

  <script src="mockup.js"></script>
</head>
<body>
  <div class="container-fluid">
    <div class="col-md-3 ">
      HOME
    </div>
    <div class="col-md-3 ">
      UPLOAD AN ENSEMBLE
    </div>
    <div class="col-md-3 ">
      MY FAVORITES
    </div>
    <div class="col-md-3 ">
      LOGOUT
    </div>

    <div class="row searchbar">
      <div class="col-md-4 col-md-offset-4" id="searchbar">
        <input class="col-12" type="search" placeholder="Search hashtags"></div>
      <div class="col-md-4 col-md-offset-4">SORT_BY FIELD</div>
    </div>

    <div class="dashboard">
      <div class="col-md-3 col-xs-6" id="recent-outfits">
        <!-- recent-outfits-template goes here.... -->
      </div>
      <div class="col-md-3 col-md-push-6 col-xs-6" id="trending-hashtags">
        <!-- trending-hashtags-template goes here... -->
      </div>
      <div class="col-md-6 col-md-pull-3 col-xs-12" id="feed">
        <div class="container">
          <div class="col-12" id="all-outfits">
            <!-- single-outfit-template goes here.... -->
          </div>
        </div>
      </div>
    </div>
  </div>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js"></script>
</body>
</html>
